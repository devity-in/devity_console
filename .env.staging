# Staging Environment Variables
API_BASE_URL=https://api.staging.example.com
API_KEY=staging_api_key_456
ENVIRONMENT=staging
APP_VERSION=3.29.0
API_VERSION=v1
ANALYTICS_ENDPOINT=https://analytics.staging.example.com/events

